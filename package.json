  "scripts": {
    "build": "gatsby build",
    "build:cache": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "test": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:e2e": "START_SERVER_AND_TEST_INSECURE=1 start-server-and-test start http://localhost:8000 cy:open",
    "test:e2e:ci": "START_SERVER_AND_TEST_INSECURE=1 start-server-and-test start http://localhost:8000 cy:run",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "clean": "gatsby clean && yarn exec rimraf ./node_modules ./coverage ./yarn-error.log",
    "lint": "eslint --ignore-path .gitignore . --ext js,jsx,ts,tsx",
    "prettify": "prettier --list-different './src/**/*.{js,jsx,json,yml,yaml,css,less,scss,ts,tsx,md,graphql,mdx}' package.json",
    "prettify:fix": "yarn prettify --write",
    "validate": "yarn lint && yarn prettify",
    "snyk-protect": "snyk protect"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/meuxx/pablo.dev"
  },
  "snyk": true
